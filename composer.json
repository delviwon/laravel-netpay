{
    "name": "lewee/netpay",
    "description": "A PHP SDK for netpay",
    "authors": [
        {
            "name": "jowan",
            "email": "jowan@ahlewei.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lewee\\Netpay\\NetpayProvider"
            ],
            "aliases": {
                "Netpay": "Lewee\\Netpay\\Facades\\Netpay"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Lewee\\Netpay\\": "src/"
        }
    }
}
